    "nanoid": "^4.0.0",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.11",
    "qs": "^6.10.3",
    "rgbaster": "^2.1.1",
    "unplugin-vue-components": "^0.18.5",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12",
